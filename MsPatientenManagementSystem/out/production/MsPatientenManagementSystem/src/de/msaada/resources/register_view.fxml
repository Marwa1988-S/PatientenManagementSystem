<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="161.0" prefWidth="403.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.msaada.maingui.UiController">

   <children>
   <Label text="Doktor Daten">
      <font>
         <Font size="14.0" />
      </font>
      <VBox.margin>
         <Insets left="30.0" />
      </VBox.margin></Label>

      <HBox prefHeight="41.0" prefWidth="403.0">
      <children>
         <TextField fx:id="txtDoctorFName" promptText="Doktor Vorname">
            <font>
               <Font size="14.0" />
            </font></TextField>
         <TextField fx:id="txtDoctorLName" promptText="Doktor Nachname">
            <font>
               <Font size="14.0" />
            </font></TextField>
      </children>
      <padding>
         <Insets left="20.0" right="20.0" />
      </padding>
      </HBox>
      <HBox prefHeight="40.0" prefWidth="403.0">
         <children>
            <TextField fx:id="txtDoctorSpecialization" promptText="Doktor Spezialisierung">
               <font>
                  <Font size="14.0" />
               </font></TextField>
            <TextField fx:id="txtDoctorEmail" promptText="Doktor Email">
               <font>
                  <Font size="14.0" />
               </font></TextField>
         </children>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
      </HBox>

      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="Doktor Zugangsdaten">
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="txtUserName" promptText="username">
               <font>
                  <Font size="14.0" />
               </font></TextField>
            <PasswordField fx:id="txtUserPw" promptText="password">
               <font>
                  <Font size="14.0" />
               </font></PasswordField>
            <Button fx:id="btnAddnewDoctor" alignment="CENTER" minWidth="200.0" mnemonicParsing="false" onAction="#registerNewDoctor" text="ok">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            <Hyperlink alignment="TOP_LEFT" onAction="#switchBackToLoggedInScreen" text="Back">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="270.0" />
               </VBox.margin>
            </Hyperlink>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="20.0" right="20.0" />
         </VBox.margin>
      </VBox>
   </children>
</VBox>
